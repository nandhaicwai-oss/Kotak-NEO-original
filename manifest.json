{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "NeoPlus For Kotak",
    "description": "Enhance the Kotak Neo web trading platform with NeoPlus!",
    "version": "1.315",
    "manifest_version": 3,
    "options_page": "Blocks/Extra/SettingPage.html",
    "action": {
        "default_popup": "Blocks/Extra/Popup.html",
        "default_icon": {
            "16": "Arts/Art_NeoPlus/icon_16.png",
            "32": "Arts/Art_NeoPlus/icon_32.png",
            "48": "Arts/Art_NeoPlus/icon_48.png",
            "64": "Arts/Art_NeoPlus/icon_64.png",
            "128": "Arts/Art_NeoPlus/icon_128.png"
        }
    },
    "icons": {
        "16": "Arts/Art_NeoPlus/icon_16.png",
        "32": "Arts/Art_NeoPlus/icon_32.png",
        "48": "Arts/Art_NeoPlus/icon_48.png",
        "64": "Arts/Art_NeoPlus/icon_64.png",
        "128": "Arts/Art_NeoPlus/icon_128.png"
    },
    "background": {
        "service_worker": "Scripts/Other/background.js"
    },
    "content_scripts": [
        {
            "js": [
                "Scripts/Plugins/helper.js",
                "Scripts/Plugins/papaParse.js",
                "Scripts/Plugins/socketIO.js",
                "Scripts/Plugins/sortableList.js",
                "Scripts/Plugins/StorageHelperChrome.js",
                "Scripts/Plugins/LightWeightChart.js",
                "Scripts/Plugins/SocketHelper.js",
                "Scripts/versionInfo.js",
                "Scripts/coreKP.js"
            ],
            "matches": [
                "https://mis.kotaksecurities.com/*",
                "https://cis.kotaksecurities.com/*",
                "https://lapi.kotaksecurities.com/*",
                "https://neo.kotaksecurities.com/*",
                "https://ntrade.kotaksecurities.com/*"
            ],
            "all_frames": false
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "Arts/*"
            ],
            "matches": [
                "https://mis.kotaksecurities.com/*",
                "https://cis.kotaksecurities.com/*",
                "https://lapi.kotaksecurities.com/*",
                "https://neo.kotaksecurities.com/*",
                "https://ntrade.kotaksecurities.com/*"
            ]
        },
        {
            "resources": [
                "Styles/*"
            ],
            "matches": [
                "https://mis.kotaksecurities.com/*",
                "https://cis.kotaksecurities.com/*",
                "https://lapi.kotaksecurities.com/*",
                "https://neo.kotaksecurities.com/*",
                "https://ntrade.kotaksecurities.com/*"
            ]
        },
        {
            "resources": [
                "Blocks/*"
            ],
            "matches": [
                "https://mis.kotaksecurities.com/*",
                "https://cis.kotaksecurities.com/*",
                "https://lapi.kotaksecurities.com/*",
                "https://neo.kotaksecurities.com/*",
                "https://ntrade.kotaksecurities.com/*"
            ]
        }
    ],
    "permissions": [
        "storage",
        "unlimitedStorage"
    ]
}